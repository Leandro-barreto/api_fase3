{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "env": {"PIP_REQUIREMENTS_FILE": "requirements-prod.txt"}
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "main.py" }
    ],
    "functions": {
      "api/**.py": {
        "maxDuration": 10,
        "memory": 512
      }
    }

}
